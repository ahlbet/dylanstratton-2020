  },
  "scripts": {
    "build": "gatsby build",
    "release": "standard-version",
    "release:push": "yarn run release && git push --follow-tags origin develop",
    "develop": "NODE_ENV=development GATSBY_USE_LOCAL_DATA=true gatsby develop",
    "develop-local": "NODE_ENV=development GATSBY_USE_LOCAL_DATA=true gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": " npm run develop",
    "start-local": "npm run develop-local",
    "serve": "gatsby clean && gatsby build && gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "build:ts": "tsc",
    "build:init": "tsc src/utils/init-script/*.ts init.ts --outDir dist --target es2020 --module commonjs",
    "dev:init": "ts-node-dev --respawn init.ts",
    "test:e2e": "E2E_BASE_URL=http://localhost:8000 npx playwright test",
    "test:e2e:headed": "E2E_BASE_URL=http://localhost:8000 npx playwright test --headed",
    "test:e2e:debug": "E2E_BASE_URL=http://localhost:8000 npx playwright test --debug",
    "test:e2e:ui": "E2E_BASE_URL=http://localhost:8000 npx playwright test --ui",
    "test:e2e:report": "npx playwright show-report",
    "test:e2e:install": "npx playwright install",
    "new-day": "yarn build:init && node ./dist/init.js",
    "new-day-dry-run": "node scripts/init-dry-run.js",
    "new-day-dry-run-ts": "ts-node init-dry-run.ts",
    "new-day-dry-run-ts-compiled": "yarn build:init && node ./dist/init-dry-run.js",
    "new-day-coherency-test": "node scripts/init-dry-run-coherency.js",
    "markov-editor": "node scripts/markov-text-editor-cli.js",
    "generate-markov": "node scripts/generate-markov-data.js",
    "clean-markov-bad-words": "node scripts/clean-markov-texts-with-bad-words.js",
    "clean-asterisks": "node scripts/clean-asterisks-from-markov-texts.js",
    "generate-local-data": "node scripts/generate-local-markov-data.js",
    "test-local-dev": "node scripts/test-local-dev.js",
    "fetch-audio": "node scripts/fetch-all-audio-from-supabase.js",
    "test-presigned-urls": "node scripts/test-presigned-urls.js",
    "test-init-local": "node scripts/test-init-local-update.js",
    "debug-local-dev": "node scripts/debug-local-dev.js",
    "test-url-conversion": "node scripts/test-url-conversion.js",
    "clean-local-markov": "node scripts/clean-local-markov-texts.js",
    "fetch-markov-texts": "node scripts/fetch-markov-texts-from-supabase.js",
    "play-audio": "node scripts/play-audio.js",
    "prepare": "husky"
  }
