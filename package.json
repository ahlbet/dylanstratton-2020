{
  "name": "gatsby-starter-blog",
  "private": true,
  "description": "A starter for a blog powered by Gatsby and Markdown",
  "version": "0.1.1",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.18",
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/list": "^6.1.18",
    "@fullcalendar/react": "^6.1.18",
    "@fullcalendar/timegrid": "^6.1.18",
    "@supabase/supabase-js": "^2.50.2",
    "bad-words": "3.0.0",
    "canvas": "^3.1.2",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "gatsby": "^5.14.3",
    "gatsby-plugin-dark-mode": "^1.1.2",
    "gatsby-plugin-feed": "^5.14.0",
    "gatsby-plugin-google-analytics": "^5.14.0",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-offline": "^6.14.0",
    "gatsby-plugin-react-helmet": "^6.14.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-plugin-typography": "^5.14.0",
    "gatsby-remark-audio": "^1.0.2",
    "gatsby-remark-copy-linked-files": "^6.14.0",
    "gatsby-remark-images": "^7.14.0",
    "gatsby-remark-prismjs": "^7.14.0",
    "gatsby-remark-responsive-iframe": "^6.14.0",
    "gatsby-remark-smartypants": "^6.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-source-supabase": "^2.1.2",
    "gatsby-transformer-remark": "^6.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.532.0",
    "p5.sound": "^0.2.0",
    "prismjs": "^1.19.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-h5-audio-player": "3.10.0-rc.1",
    "react-helmet": "^6.1.0",
    "react-typography": "^0.16.19",
    "typeface-merriweather": "1.1.13",
    "typeface-montserrat": "1.1.13",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "babel-preset-gatsby": "^3.14.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "standard-version": "^9.5.0"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "release": "standard-version",
    "release:push": "yarn run release && git push --follow-tags origin develop",
    "develop": "NODE_ENV=development GATSBY_USE_LOCAL_DATA=true gatsby develop",
    "develop-local": "NODE_ENV=development GATSBY_USE_LOCAL_DATA=true gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": " npm run develop",
    "start-local": "npm run develop-local",
    "serve": "gatsby clean && gatsby build && gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test:e2e": "E2E_BASE_URL=http://localhost:8000 npx playwright test",
    "test:e2e:headed": "E2E_BASE_URL=http://localhost:8000 npx playwright test --headed",
    "test:e2e:debug": "E2E_BASE_URL=http://localhost:8000 npx playwright test --debug",
    "test:e2e:ui": "E2E_BASE_URL=http://localhost:8000 npx playwright test --ui",
    "test:e2e:report": "npx playwright show-report",
    "test:e2e:install": "npx playwright install",
    "new-day": "node ./init.js",
    "new-day-dry-run": "node scripts/init-dry-run.js",
    "markov-editor": "node scripts/markov-text-editor-cli.js",
    "generate-markov": "node scripts/generate-markov-data.js",
    "clean-markov-bad-words": "node scripts/clean-markov-texts-with-bad-words.js",
    "clean-asterisks": "node scripts/clean-asterisks-from-markov-texts.js",
    "generate-local-data": "node scripts/generate-local-markov-data.js",
    "test-local-dev": "node scripts/test-local-dev.js",
    "fetch-audio": "node scripts/fetch-all-audio-from-supabase.js",
    "test-presigned-urls": "node scripts/test-presigned-urls.js",
    "test-init-local": "node scripts/test-init-local-update.js",
    "debug-local-dev": "node scripts/debug-local-dev.js",
    "test-url-conversion": "node scripts/test-url-conversion.js",
    "clean-local-markov": "node scripts/clean-local-markov-texts.js",
    "fetch-markov-texts": "node scripts/fetch-markov-texts-from-supabase.js",
    "prepare": "husky"
  }
}
